/* owl-carousel */
.owl-carousel {
    display: block;
    width: 100%;
    z-index: 1;
}
.owl-stage-outer{
    position: relative;
}
.owl-stage-outer .owl-nav {
    display: flex;
    justify-content: space-between;
    margin: 0 10px;
    position: absolute;
    top: 50%;
    width: 100%;
}
